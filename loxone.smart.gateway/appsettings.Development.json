{
  "Logging": {
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] ",
        "UseUtcTimestamp": false
      }
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Api": {
    "PhilipsHueConfiguration": {
      "IP": "<your-bridge-ip-here>",
      "AccessKey": "<your-access-key>"
    }
  }
}
